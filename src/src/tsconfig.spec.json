{
  "compilerOptions": {
    "downlevelIteration": true,
    "target": "es2015",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom", "es5" ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
    "exclude": [
    "node_modules"
  ]

}
